{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
      "error", { 
        "vars": "local",
        "args": "none" 
      }
    ]
  }
}